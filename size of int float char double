# size-of-variables
 sizes of variables such int float char double in bytes
 #int <iostream>
 using namespace std ;
 int main()
 {
  cout <<"size of char:"<< sizeof(char)<<"byte"<<end1;
  cout <<"size of int:"<<sizeof(Float)<<"bytes"<<end1:
  cout<<"size of double: "<<sizeof(double)<<"bytes"<<end1;
  cout <<"size of float: "<<sizeof(float)<<"bytes"<<end1;
return 0;
}
